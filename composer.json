{
    "name": "kunsal/laravel-modular",
    "description": "Create modular application scaffold in laravel 5.4+",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olakunle Salami",
            "email": "kunsal2003@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "laravelcollective/html": "5.*",
        "illuminate/database": "5.*@dev",
        "illuminate/validation": "5.*@dev",
        "kris/laravel-form-builder": "1.*",
        "yajra/laravel-datatables-oracle":"~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.4",
        "laracasts/generators": "@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.12.x-dev"
        },
        "laravel": {
            "providers": [
                "Kunsal\\LaravelModular\\ModularServiceProvider"
            ],
            "aliases": {
                "Modular": "Kunsal\\LaravelModular\\Facades\\Modular"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Kunsal\\": "src/Kunsal/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
